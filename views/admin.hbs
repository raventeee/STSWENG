<section>
    <h1>All Customers</h1>
    <div class ="container mb-3 mt-3"> 
        <table id="example" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Customer Id</th>
                    <th>Customer Email</th>
                    <th>Customer Name</th>
                </tr>
            </thead>
        <tbody>
        {{#each customers}}
                <tr>
                    <td>{{this.customerId}}</td>
                    <td>{{this.customerEmail}}</td>
                    <td>{{this.customerFirstName}} {{this.customerLastName}}</td>
                </tr>
        {{/each}}
         </tbody>
        </table>
    </div>
</section>