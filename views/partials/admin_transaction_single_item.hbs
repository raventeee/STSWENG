<!--One Entry-->
<tr>
    <td class="tid">{{this.transactId}}</td>
    <td>
        {{#each this.finalProducts}}
        <ul>{{this.productId}}</ul>
        {{/each}}
    </td>
    <td>
        {{#each this.finalProducts}}
        <ul>{{this.productName}}</ul>
        {{/each}}
    </td>
    <td>
        {{#each this.finalProducts}}
        <ul>{{this.quantity}}</ul>
        {{/each}}
    </td>
    <td>{{this.totalPrice}}</td>
    <td>
        {{#if (isPaymentPending this.orderStatus)}}
        <select class="form-select form-select-sm drop" aria-label="Default select example">
            <option value="Payment Pending" selected>Payment Pending</option>
            <option value="Payment Successful">Payment Successful</option>
            <option value="To Be Shipped">To Be Shipped</option>
            <option value="On Transit">On Transit</option>
            <option value="Delivered">Delivered</option>
            <option value="Cancelled">Cancelled</option>
        </select>
        {{else if (isPaymentSuccessful this.orderStatus)}}
        <select class="form-select form-select-sm drop" aria-label="Default select example">
            <option value="Payment Pending">Payment Pending</option>
            <option value="Payment Successful" selected>Payment Successful</option>
            <option value="To Be Shipped">To Be Shipped</option>
            <option value="On Transit">On Transit</option>
            <option value="Delivered">Delivered</option>
            <option value="Cancelled">Cancelled</option>
        </select>

        {{else if (isToBeShipped this.orderStatus)}}
        <select class="form-select form-select-sm drop" aria-label="Default select example">
            <option value="Payment Pending">Payment Pending</option>
            <option value="Payment Successful">Payment Successful</option>
            <option value="To Be Shipped" selected>To Be Shipped</option>
            <option value="On Transit">On Transit</option>
            <option value="Delivered">Delivered</option>
            <option value="Cancelled">Cancelled</option>
        </select>

        {{else if (isOnTransit this.orderStatus)}}
        <select class="form-select form-select-sm drop" aria-label="Default select example">
            <option value="Payment Pending">Payment Pending</option>
            <option value="Payment Successful">Payment Successful</option>
            <option value="To Be Shipped">To Be Shipped</option>
            <option value="On Transit" selected>On Transit</option>
            <option value="Delivered">Delivered</option>
            <option value="Cancelled">Cancelled</option>
        </select>
        {{else if (isDelivered this.orderStatus)}}
        <select class="form-select form-select-sm drop" aria-label="Default select example">
            <option value="Payment Pending">Payment Pending</option>
            <option value="Payment Successful">Payment Successful</option>
            <option value="To Be Shipped">To Be Shipped</option>
            <option value="On Transit">On Transit</option>
            <option value="Delivered" selected>Delivered</option>
            <option value="Cancelled">Cancelled</option>
        </select>
        {{else}}
        <select class="form-select form-select-sm drop" aria-label="Default select example">
            <option value="Payment Pending">Payment Pending</option>
            <option value="Payment Successful">Payment Successful</option>
            <option value="To Be Shipped">To Be Shipped</option>
            <option value="On Transit">On Transit</option>
            <option value="Delivered">Delivered</option>
            <option value="Cancelled"selected>Cancelled</option>
        </select>
        {{/if}}
    </td>
</tr>
<!--One Entry-->